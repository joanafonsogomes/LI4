@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";

    string str = (string)@ViewData["User_Name"];
}

@{
    ViewBag.Title = "Sobre Nós";
}

<head>
    <style>
        html * {
            font-family: Montserrat;
        }
    </style>
</head>

<!--Historia-->
<section class="page-section" id="about">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading text-uppercase">Sobre Nós</h2>
                <h3 class="section-subheading text-muted">Conheça a história e a equipa.</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <ul class="timeline">
                    <li>
                        <div class="timeline-image">
                            <img class="rounded-circle img-fluid" src="~/Images/1.png" alt="">
                        </div>
                        <div class="timeline-panel">
                            <div class="timeline-heading">
                                <h4>2020</h4>
                                <h4 class="subheading">De onde somos</h4>
                            </div>
                            <div class="timeline-body">
                                <p class="text-muted">Estudantes da universidade do Minho situada em Braga, Portugal!</p>
                            </div>
                        </div>
                    </li>
                    <li class="timeline-inverted">
                        <div class="timeline-image">
                            <img class="rounded-circle img-fluid" src="~/Images/2.png" alt="">
                        </div>
                        <div class="timeline-panel">
                            <div class="timeline-heading">
                                <h4>2020</h4>
                                <h4 class="subheading">Área de Apredizagem</h4>
                            </div>
                            <div class="timeline-body">
                                <p class="text-muted">Somos estudantes do curso de Engenharia Informática</p>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="timeline-image">
                            <img class="rounded-circle img-fluid" src="~/Images/3.jpg" alt="">
                        </div>
                        <div class="timeline-panel">
                            <div class="timeline-heading">
                                <h4>Fevereiro de 2020</h4>
                                <h4 class="subheading">Contextualização</h4>
                            </div>
                            <div class="timeline-body">
                                <p class="text-muted"> Este website está a ser construído por nós no âmbito da Unidade Curricular de Laboratórios de Informática IV e teve inicio em Fevereiro de 2020</p>
                            </div>
                        </div>
                    </li>
                    <li class="timeline-inverted">
                        <div class="timeline-image">
                            <img class="rounded-circle img-fluid" src="~/Images/4.png" alt="">
                        </div>
                        <div class="timeline-panel">
                            <div class="timeline-heading">
                                <h4>Junho de 2020</h4>
                                <h4 class="subheading">Objetivos</h4>
                            </div>
                            <div class="timeline-body">
                                <p class="text-muted"> Contrução de um website que permita venda e aluguer dos mais variados itens!</p>
                            </div>
                        </div>
                    </li>
                    <li class="timeline-inverted">
                        <div class="timeline-image">
                            <h4>
                                Faça parte
                                <br>da nossa
                                <br>história!
                            </h4>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</section>

